{
  "name": "@aics/fms-file-explorer-core",
  "version": "1.0.0-alpha.0",
  "description": "NPM library that exposes the core React/Redux FMS File Explorer application. Intended to be rendered into a container.",
  "main": "nodejs/index.js",
  "module": "web/index.js",
  "types": "type-declarations",
  "repository": {
    "type": "git",
    "url": "https://aicsbitbucket.corp.alleninstitute.org/projects/SW/repos/fms-file-explorer/browse"
  },
  "files": [
    "assets",
    "web",
    "nodejs",
    "type-declarations",
    "README.md",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm-run-all --print-label clean --parallel generateTypes transpile:ts:nodejs transpile:ts:web transpile:pcss",
    "build:watch": "npm-run-all --print-label clean transpile:pcss --parallel generateTypes:watch transpile:ts:nodejs:watch transpile:ts:web:watch transpile:pcss:watch",
    "clean": "rimraf nodejs web type-declarations",
    "format": "prettier --write src/**/*",
    "generateTypes": "tsc --emitDeclarationOnly",
    "generateTypes:watch": "npm run generateTypes -- --watch",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "precommit": "lint-staged",
    "prepare": "npm run build",
    "publishArtifact": "npm publish",
    "test": "BABEL_ENV=commonjs TS_NODE_FILES=true TS_NODE_PROJECT=tsconfig.commonjs.json mocha --opts mocha.opts --recursive src/**/*.test.*",
    "transpile:pcss": "postcss \"src/**/*.css\" --base src --dir web --verbose",
    "transpile:pcss:single": "postcss --base src --dir web --verbose",
    "transpile:pcss:watch": "chokidar --debounce=1000 \"src/**/*.css\" -c \"if [ '{event}' != 'unlink' ]; then npm run transpile:pcss:single -- {path}; fi;\"",
    "transpile:ts:nodejs": "babel src --env-name nodejs --out-dir nodejs --extensions .ts,.tsx --ignore \"src/**/*.test.ts\" --source-maps",
    "transpile:ts:nodejs:watch": "npm run transpile:ts:nodejs -- --watch --verbose",
    "transpile:ts:web": "babel src --out-dir web --extensions .ts,.tsx --ignore \"src/**/*.test.ts\" --source-maps",
    "transpile:ts:web:watch": "npm run transpile:ts:web -- --watch --verbose",
    "typeCheck": "tsc --noEmit"
  },
  "author": "Gabe Medrash<gabeme@alleninstitute.org>",
  "license": "2-clause BSD license plus a third clause that prohibits redistribution and use for commercial purposes without further permission.",
  "devDependencies": {
    "@babel/cli": "7.6.0",
    "@babel/core": "7.6.0",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-optional-chaining": "7.8.3",
    "@babel/preset-env": "7.6.0",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.8.3",
    "@testing-library/react": "9.4.0",
    "@types/chai": "4.2.2",
    "@types/classnames": "2.2.9",
    "@types/debounce-promise": "3.1.1",
    "@types/enzyme": "3.10.3",
    "@types/jsdom": "12.2.4",
    "@types/lodash": "4.14.138",
    "@types/lru-cache": "5.1.0",
    "@types/mocha": "5.2.7",
    "@types/react": "16.9.43",
    "@types/react-beautiful-dnd": "11.0.3",
    "@types/react-redux": "7.1.4",
    "@types/react-window": "1.8.1",
    "@types/react-window-infinite-loader": "1.0.1",
    "@types/redux": "3.6.0",
    "@types/sinon": "7.0.13",
    "@typescript-eslint/eslint-plugin": "2.6.0",
    "@typescript-eslint/parser": "2.6.0",
    "babel-plugin-import": "1.12.1",
    "chai": "4.2.0",
    "chokidar-cli": "2.1.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.5.0",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.2.0",
    "jsdom": "16.0.1",
    "lint-staged": "9.2.5",
    "mocha": "6.2.0",
    "mock-css-modules": "1.0.0",
    "npm-run-all": "4.1.5",
    "postcss-cli": "6.1.3",
    "prettier": "1.19.1",
    "rimraf": "3.0.0",
    "sinon": "7.4.2",
    "ts-node": "8.4.1",
    "typescript": "3.7.5"
  },
  "dependencies": {
    "@aics/redux-utils": "0.3.1",
    "@tippy.js/react": "3.1.1",
    "axios": "0.19.0",
    "classnames": "2.2.6",
    "cockatiel": "0.1.3",
    "debounce-promise": "3.1.2",
    "filesize": "5.0.3",
    "fuse.js": "3.4.6",
    "interactjs": "1.6.2",
    "lodash": "4.17.15",
    "lru-cache": "5.1.1",
    "normalize.css": "8.0.1",
    "office-ui-fabric-react": "7.84.1",
    "react": "16.9.0",
    "react-beautiful-dnd": "12.1.1",
    "react-redux": "7.1.1",
    "react-window": "1.8.5",
    "react-window-infinite-loader": "1.0.5",
    "redux": "4.0.4",
    "redux-logic": "2.1.1",
    "reselect": "4.0.0",
    "resize-observer": "1.0.0"
  }
}

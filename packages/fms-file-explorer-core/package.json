{
  "name": "@aics/fms-file-explorer-core",
  "version": "4.2.2",
  "description": "NPM library that exposes the core React/Redux FMS File Explorer application. Intended to be rendered into a container.",
  "main": "nodejs/index.js",
  "module": "web/index.js",
  "types": "type-declarations",
  "repository": {
    "type": "git",
    "url": "https://aicsbitbucket.corp.alleninstitute.org/projects/SW/repos/fms-file-explorer/browse"
  },
  "files": [
    "web",
    "nodejs",
    "type-declarations",
    "README.md",
    "LICENSE.txt",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm-run-all --print-label clean --parallel generateTypes transpile:ts:nodejs transpile:ts:web transpile:pcss",
    "build:watch": "npm-run-all --print-label clean transpile:pcss --parallel generateTypes:watch transpile:ts:nodejs:watch transpile:ts:web:watch transpile:pcss:watch",
    "clean": "rimraf nodejs web type-declarations",
    "generateTypes": "tsc --emitDeclarationOnly",
    "generateTypes:watch": "npm run generateTypes -- --watch",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "npm run build",
    "publishArtifact": "npm publish",
    "test": "BABEL_ENV=test mocha",
    "transpile:pcss": "postcss \"src/**/*.css\" --base src --dir web --verbose",
    "transpile:pcss:single": "postcss --base src --dir web --verbose",
    "transpile:pcss:watch": "chokidar --debounce=1000 \"src/**/*.css\" -c \"if [ '{event}' != 'unlink' ]; then npm run transpile:pcss:single -- {path}; fi;\"",
    "transpile:ts:nodejs": "babel src --env-name nodejs --out-dir nodejs --extensions .ts,.tsx --ignore \"src/**/*.test.ts\" --source-maps",
    "transpile:ts:nodejs:watch": "npm run transpile:ts:nodejs -- --watch --verbose",
    "transpile:ts:web": "babel src --out-dir web --extensions .ts,.tsx --ignore \"src/**/*.test.ts\" --source-maps",
    "transpile:ts:web:watch": "npm run transpile:ts:web -- --watch --verbose",
    "typeCheck": "tsc --noEmit"
  },
  "author": "Allen Institute for Cell Science",
  "license": "SEE LICENSE IN LICENSE.txt",
  "devDependencies": {
    "@babel/cli": "7.6.x",
    "@babel/core": "7.6.x",
    "@babel/plugin-proposal-class-properties": "7.5.x",
    "@babel/plugin-proposal-optional-chaining": "7.8.x",
    "@babel/polyfill": "7.10.x",
    "@babel/preset-env": "7.6.x",
    "@babel/preset-react": "7.0.x",
    "@babel/preset-typescript": "7.8.x",
    "@babel/register": "7.10.x",
    "@testing-library/react": "11.1.x",
    "@types/chai": "4.2.x",
    "@types/classnames": "2.2.x",
    "@types/debounce-promise": "3.1.x",
    "@types/enzyme": "3.10.x",
    "@types/jsdom": "12.2.x",
    "@types/lodash": "4.14.x",
    "@types/lru-cache": "5.1.x",
    "@types/mocha": "8.0.x",
    "@types/react": "16.9.x",
    "@types/react-beautiful-dnd": "11.0.x",
    "@types/react-redux": "7.1.x",
    "@types/react-syntax-highlighter": "13.5.x",
    "@types/react-window": "1.8.x",
    "@types/react-window-infinite-loader": "1.0.x",
    "@types/redux": "3.6.x",
    "@types/sinon": "7.0.x",
    "@typescript-eslint/eslint-plugin": "2.6.x",
    "@typescript-eslint/parser": "2.6.x",
    "babel-plugin-import": "1.12.x",
    "chai": "4.2.x",
    "chokidar-cli": "2.1.x",
    "css-modules-require-hook": "4.2.x",
    "enzyme": "3.10.x",
    "enzyme-adapter-react-16": "1.14.x",
    "eslint": "6.6.x",
    "eslint-config-prettier": "6.5.x",
    "eslint-plugin-react": "7.16.x",
    "eslint-plugin-react-hooks": "2.2.x",
    "jsdom": "16.4.x",
    "mocha": "8.1.x",
    "mock-raf": "1.0.x",
    "npm-run-all": "4.1.x",
    "postcss-cli": "6.1.x",
    "rimraf": "3.0.x",
    "sinon": "7.4.x",
    "ts-node": "8.4.x",
    "typescript": "3.8.x"
  },
  "dependencies": {
    "@aics/redux-utils": "0.5.x",
    "@fluentui/react": "7.154.x",
    "@tippy.js/react": "3.1.x",
    "axios": "0.19.x",
    "classnames": "2.2.x",
    "cockatiel": "0.1.x",
    "debounce-promise": "3.1.x",
    "filesize": "5.0.x",
    "fuse.js": "3.4.x",
    "interactjs": "1.6.x",
    "lodash": "4.17.x",
    "lru-cache": "5.1.x",
    "normalize.css": "8.0.x",
    "react": "16.9.x",
    "react-beautiful-dnd": "12.1.x",
    "react-redux": "7.1.x",
    "react-syntax-highlighter": "15.3.x",
    "react-window": "1.8.x",
    "react-window-infinite-loader": "1.0.x",
    "redux": "4.0.x",
    "redux-logic": "2.1.x",
    "reselect": "4.0.x",
    "resize-observer": "1.0.x",
    "string-natural-compare": "3.0.x"
  }
}

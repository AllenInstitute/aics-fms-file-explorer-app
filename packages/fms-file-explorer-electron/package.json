{
  "name": "fms-file-explorer-electron",
  "version": "1.1.0",
  "description": "An application designed to simplify access and exploration of data produced by the Allen Institute for Cell Science, provide an intuitive mechanism for organizing that data, and provide simple hooks for incorporating that data into both programmatic and non-programmatic workflows.",
  "main": "dist/main/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/AllenInstitute/aics-fms-file-explorer-app.git"
  },
  "build": {
    "appId": "org.aics.alleninstitute.fileexplorer",
    "directories": {
      "buildResources": "assets",
      "output": "build"
    },
    "files": [
      "assets/*",
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "linux": {
      "target": "AppImage"
    },
    "mac": {
      "target": "dmg"
    },
    "publish": {
      "provider": "github"
    },
    "productName": "FMS File Explorer"
  },
  "scripts": {
    "build": "npm-run-all --print-label clean makeDistDir --parallel bundle transpile",
    "bundle": "webpack --config ./webpack/webpack.config.js --env.env production",
    "clean": "rimraf dist",
    "format": "prettier --write src/**/*",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "makeDistDir": "mkdir -p dist",
    "precommit": "lint-staged",
    "publishArtifact": "echo 'npm run publishArtifact is a noop for fms-file-explorer-electron'",
    "start": "node ./scripts/start-dev.js",
    "test": "BABEL_ENV=nodejs mocha",
    "transpile": "babel src --ignore \"src/renderer.tsx\" --ignore \"src/test/**/*\" --source-maps --extensions .ts,.tsx,.js,.jsx --env-name nodejs --out-dir dist",
    "typeCheck": "tsc -p tsconfig.json --noEmit"
  },
  "author": "Gabe Medrash<gabeme@alleninstitute.org>",
  "license": "2-clause BSD license plus a third clause that prohibits redistribution and use for commercial purposes without further permission.",
  "devDependencies": {
    "@babel/cli": "7.6.0",
    "@babel/core": "7.6.0",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/preset-env": "7.6.0",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.8.3",
    "@types/chai": "4.2.2",
    "@types/classnames": "2.2.9",
    "@types/electron-devtools-installer": "2.2.0",
    "@types/enzyme": "3.10.3",
    "@types/jsdom": "12.2.4",
    "@types/lodash": "4.14.138",
    "@types/mocha": "8.0.2",
    "@types/react": "16.9.43",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.4",
    "@types/semver": "7.3.2",
    "@types/sinon": "7.0.13",
    "@typescript-eslint/eslint-plugin": "2.6.0",
    "@typescript-eslint/parser": "2.6.0",
    "babel-loader": "8.0.6",
    "babel-plugin-import": "1.12.1",
    "chai": "4.2.0",
    "clean-webpack-plugin": "3.0.0",
    "core-js": "3.3.2",
    "css-loader": "3.2.0",
    "electron": "9.1.1",
    "electron-builder": "22.2.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.5.0",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.2.0",
    "fork-ts-checker-webpack-plugin": "1.5.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.0.5",
    "jsdom": "16.0.1",
    "lint-staged": "9.2.5",
    "mini-css-extract-plugin": "0.8.0",
    "mocha": "8.1.1",
    "npm-run-all": "4.1.5",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "prettier": "1.19.1",
    "rimraf": "3.0.0",
    "sinon": "7.4.2",
    "ts-node": "8.4.1",
    "typescript": "3.7.5",
    "webpack": "4.40.2",
    "webpack-bundle-analyzer": "3.5.0",
    "webpack-cli": "3.3.8",
    "webpack-dev-server": "3.8.0"
  },
  "dependencies": {
    "@aics/fms-file-explorer-core": "1.1.0",
    "electron-devtools-installer": "3.1.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-redux": "7.1.1",
    "regenerator-runtime": "0.13.3",
    "semver": "7.3.2"
  },
  "resolutions": {
    "@types/node": "12.x",
    "@types/react": "16.9.43",
    "@types/react-dom": "16.9.8"
  }
}

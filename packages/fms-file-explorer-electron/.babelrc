{
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    ["babel-plugin-import", {
      "camel2DashComponentName": false,
      "libraryDirectory": "",
      "libraryName": "lodash"
    }, "lodash"]
  ],
  "presets": [
    "@babel/preset-react",
    ["@babel/preset-env", {
        "corejs": 3,
        "modules": false,
        "useBuiltIns": "usage"
    }],
    "@babel/preset-typescript"
  ],
  "env": {
    "nodejs": {
      "presets": [["@babel/preset-env", { "modules": "commonjs", "targets": "node 12" }]]
    }
  }
}